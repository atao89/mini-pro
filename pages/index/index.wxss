.index_swiper swiper {
  width: 100%;
  height: 300rpx;
}

.index_swiper navigator {
  flex: 1;
  height: 100%;
}

.index_swiper navigator image {
  width: 100%;
  height: 100%;
}

.index_nav {
  width: 750rpx;
  /* height: 150rpx; */
  display: flex;
}
.index_nav navigator {
  flex: 1;
  text-align: center;
  padding: 20rpx;
}
.index_nav navigator .iconfont {
  font-size: 120rpx;
  color: #EB4450;
  margin-bottom: 20rpx;
}
.index_nav navigator text {
  font-size: 30rpx;
  color: #333;
}