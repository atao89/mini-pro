/**index.wxss**/
/* 使用主题颜色 */
/* .index {
  color: var(--themeColor);
} */

.index_swiper swiper {
  width: 100%;
  height: 300rpx;
}

.index_swiper navigator {
  flex: 1;
  height: 100%;
}

.index_swiper navigator image {
  width: 100%;
  height: 100%;
}

.index_nav {
  width: 750rpx;
  /* height: 150rpx; */
  display: flex;
}
.index_nav navigator {
  flex: 1;
  text-align: center;
  padding: 20rpx;
}
.index_nav navigator .iconfont {
  font-size: 120rpx;
  color: #EB4450;
  margin-bottom: 20rpx;
}
.index_nav navigator text {
  font-size: 30rpx;
  color: #333;
}


.index_floor .floor {
  overflow: hidden;
}
.index_floor .floor .floor_title {
  padding: 15rpx;
}
.index_floor .floor .floor_title_text {
  font-size: 36rpx;
  font-weight: bold;
  color: #fff;
  padding: 0 10rpx;
  border-radius: 20rpx;
  background: rgb(248, 177, 130);
}
/* .index_floor .floor .floor_title image {
  width: 100%;
} */
.index_floor .floor .floor_list navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor .floor_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  padding-left: 10rpx;
}
.index_floor .floor .floor_list navigator:nth-child(4),
.index_floor .floor .floor_list navigator:nth-child(5) {
  padding-bottom: 15rpx;
}
.index_floor .floor .floor_list navigator:nth-child(2),
.index_floor .floor .floor_list navigator:nth-child(3) {
  padding-bottom: 10rpx;
}
.index_floor .floor .floor_list navigator:nth-child(1) {
  padding-left: 10rpx;
}
.index_floor .floor .floor_list navigator:nth-child(3),
.index_floor .floor .floor_list navigator:nth-child(5) {
  padding-right: 10rpx;
}
.index_floor .floor .floor_list navigator image {
  width: 100%;
  height: 100%;
}